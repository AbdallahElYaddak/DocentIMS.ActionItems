<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="DocentIMS.ActionItems"
      metal:use-macro="context/main_template/macros/master">
<body>
  <metal:content-core fill-slot="content-core">
  <metal:block define-macro="content-core">
    <tal:check tal:condition="not: context/portal_membership/isAnonymousUser" >
     <div tal:replace="structure  context/frontpage_text/output|None" />
   
     </tal:check>
     <div tal:replace="structure  context/frontpage_anon/output|None" tal:condition="context/portal_membership/isAnonymousUser" />
  </metal:block>
  </metal:content-core>
</body>
</html>
