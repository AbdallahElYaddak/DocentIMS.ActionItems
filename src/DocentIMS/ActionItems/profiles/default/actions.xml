<?xml version="1.0" encoding="utf-8"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="user" meta_type="CMF Action Category">
  <object name="projectmanager_controlpanel" meta_type="CMF Action"
     i18n:domain="plone">
   <property name="title"
      i18n:translate="">Project Manager Settings</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${portal_url}/@@dims_controlpanel</property>
   <property name="link_target"></property>
   <property name="icon_expr">string:plone-controlpanel</property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="DocentIMS.ActionItems: Edit Controlpanel"/>
   </property>
   <property name="visible">True</property>
  </object>

 <object name="login" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Log in</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">python:f&quot;{plone_portal_state.navigation_root_url()}/login&quot;</property>
   <property name="link_target"></property>
   <property name="icon_expr">string:plone-login</property>
   <property name="available_expr">python:member is None</property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
   <property name="modal" type="text"></property>
  </object>

  <object name="users_group" meta_type="CMF Action"
     i18n:domain="plone">
   <property name="title"
      i18n:translate="">Users and Groups</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${portal_url}/@@usergroup-userprefs</property>
   <property name="link_target"></property>
   <property name="icon_expr">string:plone-controlpanel</property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="Plone Site Setup: Users and Groups"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
 

</object>


