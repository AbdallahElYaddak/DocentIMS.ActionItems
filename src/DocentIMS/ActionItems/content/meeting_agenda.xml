<model
    xmlns="http://namespaces.plone.org/supermodel/schema"
    xmlns:security="http://namespaces.plone.org/supermodel/security"
    xmlns:form="http://namespaces.plone.org/supermodel/form"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="DocentIMS.ActionItems">
  <schema>

    <field name="parent_id" type="zope.schema.TextLine">
            <required>False</required>
            <form:widget mode="hidden" />
    </field>
    <field name="file" type="plone.namedfile.field.NamedBlobFile">
      <description>The Word file for the document created with Docent local</description>
      <required>False</required>
      <title>Word File</title>
    </field>
  </schema>
</model>
